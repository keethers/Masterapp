layout.js
import React from 'react';
import { Link } from 'react-router-dom';
import { createPageUrl } from '@/utils';
import { Home, Search, BarChart2, Shield, Gem, Briefcase, DollarSign, Power, UserCircle, Car, Plane, Building, Rocket, Utensils, Bed, Receipt } from 'lucide-react';
import { Button } from '@/components/ui/button';
import { Input } from '@/components/ui/input';
import { User } from '@/entities/User';

export default function Layout({ children, currentPageName }) {
  const handleLogout = async () => {
    await User.logout();
  };

  const navItems = [
    { page: 'MainPage', icon: <Home className="w-7 h-7" />, label: 'Main Hub' },
    { type: 'divider' },
    { page: 'TradingRoom', icon: <BarChart2 className="w-7 h-7 text-green-400" />, label: 'Trading' },
    { page: 'LegalServices', icon: <Shield className="w-7 h-7 text-blue-400" />, label: 'Legal' },
    { page: 'LuxuryMarket', icon: <Gem className="w-7 h-7 text-purple-400" />, label: 'Luxury' },
    { type: 'divider' },
    { page: 'ServicesHub', icon: <Briefcase className="w-7 h-7 text-orange-400" />, label: 'Services' },
    { page: 'TravelBundles', icon: <Plane className="w-7 h-7 text-cyan-400" />, label: 'Travel' },
    { type: 'divider' },
    { page: 'VehicleDealer', icon: <Car className="w-7 h-7 text-red-400" />, label: 'Vehicles' },
    { page: 'RealEstate', icon: <Building className="w-7 h-7 text-yellow-400" />, label: 'Real Estate' },
     { type: 'divider' },
    { page: 'ExoticaMaster', icon: <UserCircle className="w-7 h-7 text-white" />, label: 'Master' },
  ];

  return (
    <div className="min-h-screen bg-black text-white">
      <header className="fixed top-0 left-0 right-0 bg-black/80 backdrop-blur-md border-b border-purple-500/30 p-4 z-50">
        <div className="max-w-8xl mx-auto flex items-center justify-between">
          <Link to={createPageUrl('MainPage')} className="flex items-center gap-3">
            <img 
              src="https://qtrypzzcjebvfcihiynt.supabase.co/storage/v1/object/public/base44-prod/public/8469f8056_ChatGPTImageJul8202504_37_28AM.png"
              alt="ExoticaCrypto Logo - Patent Pending"
              className="w-16 h-16 object-contain rounded-xl shadow-2xl hover:scale-105 transition-transform"
            />
            <div className="flex flex-col">
              <div className="text-3xl font-bold bg-gradient-to-r from-yellow-400 to-orange-500 bg-clip-text text-transparent">
                EXOTICA
              </div>
              <div className="text-xs text-yellow-400 font-semibold">
                To The Moon. No Parachute.
              </div>
            </div>
          </Link>
          
          <div className="flex-1 flex justify-center px-8">
             <div className="relative w-full max-w-xl">
              <Search className="absolute left-4 top-1/2 transform -translate-y-1/2 text-gray-400 w-5 h-5" />
              <Input
                placeholder="Search the Exotica universe..."
                className="w-full pl-12 pr-4 py-3 bg-gray-900/50 border-2 border-purple-500/50 text-white placeholder-gray-400 rounded-full focus:ring-2 focus:ring-purple-400 transition-all"
              />
            </div>
          </div>
          
          <Button onClick={handleLogout} variant="ghost" className="text-gray-400 hover:text-white">
            <Power className="w-5 h-5" />
          </Button>
        </div>
      </header>

      <div className="fixed left-0 top-24 h-[calc(100vh-6rem)] w-24 bg-black/60 backdrop-blur-sm border-r border-purple-500/30 z-40 flex flex-col items-center">
        <nav className="flex flex-col items-center py-6 space-y-2 w-full">
          {navItems.map((item, index) => (
            item.type === 'divider' ? 
            <div key={`divider-${index}`} className="w-1/2 h-px bg-purple-500/30 my-2"></div> :
            <Link 
              key={item.page} 
              to={createPageUrl(item.page)} 
              title={item.label}
              className={`p-4 rounded-lg w-full text-center transition-colors ${currentPageName === item.page ? 'bg-purple-600/50' : 'hover:bg-purple-600/30'}`}
            >
              {item.icon}
            </Link>
          ))}
        </nav>
      </div>

      <main className="ml-24 pt-28 p-6">
        {children}
      </main>
    </div>
  );
}
